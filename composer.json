{
    "name": "nevertocry/laravel6-chumper-zipper",
    "type": "library",
    "description": "This is a little neat helper for the ZipArchive methods with handy functions",
    "keywords": ["laravel", "ZIP", "Archive"],
    "homepage": "https://github.com/nevertocry/laravel6-chumper-zipper.git",
    "license": "Apache2",
    "authors": [
        {
            "name": "Damon",
            "email": "516066509@qq.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "ext-zip": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "Chumper\\Zipper\\": "src/Chumper/Zipper"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chumper\\Zipper\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chumper\\Zipper\\ZipperServiceProvider"
            ],
            "aliases": {
                "Zipper": "Chumper\\Zipper\\Zipper"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
